{
  "name": "@alquilapp-backoffice/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build:all": "npx nx run-many --target=build --all --configuration=production",
    "build:api": "npx nx build api",
    "build:app": "npx nx build app",
    "docker:build:api:dev": "docker build -f api/Dockerfile -t alquilapp_bo_api_dev .",
    "docker:build:api:prod": "docker build -f api/Dockerfile -t alquilapp_bo_api_prod .",
    "docker:compose:api:dev": "docker compose -f docker-compose.dev.yml up -d --build",
    "docker:compose:api:prod": "docker compose -f docker-compose.prod.yml up -d --build",
    "eslint:api": "npx nx lint api",
    "eslint:app": "npx nx lint app",
    "format": "prettier --write \"**/*.ts\"",
    "prepare": "husky",
    "start:dev:all": "npx nx run-many --parallel --target=serve --projects=api,app --watch",
    "start:dev:api": "npx nx serve api --watch",
    "start:dev:app": "npx nx serve app",
    "start:prod:api": "node dist/api/main.js"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "~18.0.0",
    "@angular-devkit/core": "~18.0.0",
    "@angular-devkit/schematics": "~18.0.0",
    "@angular-eslint/eslint-plugin": "^18.0.1",
    "@angular-eslint/eslint-plugin-template": "^18.0.1",
    "@angular-eslint/template-parser": "^18.0.1",
    "@angular/cli": "~18.0.0",
    "@angular/compiler-cli": "~18.0.0",
    "@angular/language-service": "~18.0.0",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-angular": "^19.3.0",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.2",
    "@nx/angular": "19.4.0",
    "@nx/eslint": "19.4.0",
    "@nx/eslint-plugin": "19.4.0",
    "@nx/jest": "19.4.0",
    "@nx/js": "19.4.0",
    "@nx/nest": "19.4.0",
    "@nx/node": "19.4.0",
    "@nx/web": "19.4.0",
    "@nx/webpack": "19.4.0",
    "@nx/workspace": "19.4.0",
    "@schematics/angular": "~18.0.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/jest": "^29.4.0",
    "@types/node": "18.16.9",
    "@typescript-eslint/eslint-plugin": "^7.3.0",
    "@typescript-eslint/parser": "^7.3.0",
    "@typescript-eslint/utils": "^8.0.0-alpha.28",
    "eslint": "~8.57.0",
    "eslint-config-prettier": "^9.0.0",
    "husky": "^9.0.11",
    "jest": "^29.4.1",
    "jest-environment-jsdom": "^29.4.1",
    "jest-environment-node": "^29.4.1",
    "jest-preset-angular": "~14.1.0",
    "nx": "19.4.0",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "typescript": "~5.4.2",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@angular/animations": "~18.0.0",
    "@angular/cdk": "^18.0.5",
    "@angular/common": "~18.0.0",
    "@angular/compiler": "~18.0.0",
    "@angular/core": "~18.0.0",
    "@angular/forms": "~18.0.0",
    "@angular/material": "^18.0.5",
    "@angular/platform-browser": "~18.0.0",
    "@angular/platform-browser-dynamic": "~18.0.0",
    "@angular/router": "~18.0.0",
    "@nestjs/common": "^10.0.2",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.2",
    "@nestjs/platform-express": "^10.0.2",
    "axios": "^1.6.0",
    "dotenv": "^16.4.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "lint-staged": {
    "**/*.ts": [
      "npm run format",
      "eslint --max-warnings 0 ."
    ]
  }
}
